{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Go Web App",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "env": {
        "PORT": "8080"
      },
      "args": [],
      "showLog": true
    },
    {
      "name": "Attach to Go",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${command:pickProcess}"
    }
  ]
}
